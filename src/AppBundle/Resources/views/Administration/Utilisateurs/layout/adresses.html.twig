{% extends '::layout/layoutAdmin.html.twig' %}

{% block body %}
    <h1 xmlns="http://www.w3.org/1999/html">liste des  Adresse de Mr/Mme {{ utilisateur.username }}  </h1>

        {% for adresss  in utilisateur.adresses %}
       <div class="span12" >
            <div class="span4" >
                <h4>Adresse {{ loop.index }}:</h4>
                {{ adresss.adresse }} , {{ adresss.cp }} {{ adresss.ville }} - {{ adresss.pays }}
            <br>
                {{ adresss.prenom }} {{ adresss.nom }}
            </br> </br>
            </div>
            </div>
        {% endfor %}

{% endblock %}
